<Window x:Class="timp_lab2.MenuWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="MenuWindow" Height="450" Width="800" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.Background>
            <SolidColorBrush Color="{DynamicResource _Light2}"/>
        </Grid.Background>
        <DockPanel>
            <Menu Name="_menu" DockPanel.Dock="Top" Foreground="Black" FontSize="14">

            </Menu>
            <StackPanel/>
        </DockPanel>
    </Grid>
</Window>
